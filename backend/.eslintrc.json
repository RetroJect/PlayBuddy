{
    "env": {
        "es2020": true,
        "node": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "jest",
        "lcom"
    ],
    "rules": {
        "no-console": "off",
        "lcom/lcom4": "warn"
    }
}
